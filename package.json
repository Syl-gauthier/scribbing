{
  "name": "scribbing",
  "version": "0.0.1",
  "description": "Language learning focused on user helping eachover",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build:css && npm run build:js",
    "build:css": "node-sass public/style/style.scss public/style/style.css",
    "build:js": "browserify src/react/reactApp.jsx -t babelify -o public/js/bundle.js --debug",
    "build": "npm run build:css && npm run build:js",
    "prebuild": "rimraf public/js/bundle.js public/style/style.css",
    "postbuild": "watchify src/react/reactApp.jsx -t babelify -o public/js/bundle.js -v --debug & node-sass -w public/style/style.scss public/style/style.css",
    "start": "nodemon app.js",
    "start:dev": "nodemon app.js & npm run postbuild",
    "test": "mocha public/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Syl-gauthier/scribbing.git"
  },
  "keywords": [
    "language",
    "list",
    "learning"
  ],
  "author": "Sylvain Gauthier",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Syl-gauthier/scribbing/issues"
  },
  "homepage": "https://github.com/Syl-gauthier/scribbing#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "mongodb": "^2.2.29",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-rc.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "request": "^2.81.0",
    "save": "^2.3.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.0.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "watchify": "^3.9.0"
  },
  "babel": {
    "presets": [
      "env",
      "es2015",
      "react"
    ]
  }
}
